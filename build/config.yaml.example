mqtt: 
  host: mqtt.example.com
  port: 1883
  authentication:
    username: testuser
    password: testpassword


sensor: 
  - device:
      id: 183
      protocol: fineoffset
      model: temperaturehumidity
    mqttRoom: bedroom
 

